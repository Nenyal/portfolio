generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Header {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  description String
  image String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
