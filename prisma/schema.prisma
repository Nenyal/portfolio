generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type HeaderList {
  link String
  name String
  show Boolean
}

type HeaderTitle {
  link String
  name String
}

model Header {
  id    String       @id @default(auto()) @map("_id") @db.ObjectId
  id_   Int          @map("id")
  list  HeaderList[]
  title HeaderTitle
}
